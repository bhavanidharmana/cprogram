#include <stdio.h>

int main()
{
    int a[10][10],i,j,r,c,col,sum=0;
    scanf("%d%d",&r,&c);
    for(i=0;i<r;i++)
    {
    for(j=1;j<=c;j++)
    scanf("%d",&a[i][j]);
    }
    scanf("%d",&col);
    for(i=0;i<r;i++)
    {
    for(j=col;j<=col;j++)
    sum=sum+a[i][j];
    }
    printf("%d",sum);
    return 0;
}
